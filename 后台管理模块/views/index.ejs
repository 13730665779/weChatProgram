    <script>
        $(function () {
            $(".nav_tab").click(function () {
                if(!$(this).hasClass('active')){
                    $(this).addClass('active');
                    $(this).siblings().removeClass('active');
                }
                if($(this).hasClass('nav_tab01')){
                    $('.StyleInfor').show();
